@model IEnumerable<APPR6312_POE.Models.Users>

@{
    ViewData["Title"] = "Index";
}
<br />
<h1>User Information</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                <center>
                <a asp-action="Edit" class="btn btn-primary" asp-route-id="@item.email">Edit</a> 
                <a asp-action="Details" class="btn btn-primary" asp-route-id="@item.email">Details</a> 
                </center>
            </td>
        </tr>
}
    </tbody>
</table>
